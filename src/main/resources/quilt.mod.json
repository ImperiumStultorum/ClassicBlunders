{
	"schema_version": 1,
	"quilt_loader": {
		"group": "com.stultorum",
		"id": "blunders",
		"version": "${version}",
		"metadata": {
			"name": "Classic Blunders",
			"description": "A library mod for simple shared functionality between Imperium mods.",
			"contributors": {
				"CoolandonRS": "Owner"
			},
			"contact": {
				"homepage": "https://imperium.stultorum.com/",
				"issues": "https://github.com/ImperiumStultorum/ClassicBlunders/issues",
				"sources": "https://github.com/ImperiumStultorum/ClassicBlunders"
			},
			"icon": "assets/blunders/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": {
				"adapter": "kotlin",
				"value": "com.stultorum.blunders.ClassicBlunders"
			}
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.23.1-"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=6.0.0-"
			},
			{
				"id": "qkl",
				"versions": ">=2.0.2-"
			},
			{
				"id": "minecraft",
				"versions": ">=1.19.4"
			}
		]
	},
	"mixin": "blunders.mixins.json",
	"access_widener": "blunders.accesswidener"
}
